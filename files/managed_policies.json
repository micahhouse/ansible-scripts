{
  "DefaultCookiesSetting": 4
  "DefaultImagesSetting": 1
  "DefaultJavaScriptSetting": 1
  "DefaultPluginsSetting": 1
  "DefaultPopupsSetting": 1
  "DefaultNotificationsSetting": 2
